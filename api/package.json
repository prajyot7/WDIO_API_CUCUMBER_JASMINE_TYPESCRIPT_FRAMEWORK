    "@wdio/mocha-framework": "^7.10.0",
    "@wdio/shared-store-service": "8.5.7",
    "@wdio/spec-reporter": "^8.4.0",
    "allure-commandline": "^2.21.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
