{
  "name": "WDIO_CUCUMBER_JASMINE_TYPESCRIPT_FRAMEWORK",
  "version": "1.0.0",
  "description": "WebdriverIO Cucumber Jasmine Typescript Framework",
  "main": "index.js",
  "scripts": {
    "allure:generate": "allure generate logs/results/allure-results --clean -o logs/results/allure-report",
    "allure:open": "allure open logs/results/allure-report",
    "pretest": "rimraf logs/results/allure-results && rimraf logs/results/allure-report",
    "tsc": "tsc",
    "clean": "clean",
    "clean:test": "node ./node_modules/glup/bin/glup cleanTest",
    "wdio:test": " npm run pretest && npm run wdio_api:run && npm run allure:generate && npm run allure:open",
    "wdio:test_with_params": " npm run pretest && npm run wido_with_params",
    "wdio_api:run": "wdio run ./wdio.conf.api.ts",
    "wdio_api_su:run": "npm run pretest && npm run wdio_api:run"
  },
  "author": "PRAJYOT FULSUNDAR",
  "license": "ISC",
